{
    "configurations": [
        {
            "name": "tail",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/logiora",
            "env": {
                "SC_LOGIORA_PARSE_CMD": "parse ~/Proyectos/packagesGit/casamau/cmo-jumbleBox/logs/access.log"
            },
            "args": [
                "tail",
                "../../logs/short_access.log"
            ],
            "showLog": true
        },{
            "name": "parse",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/logiora",
            "env": {
                "SC_LOGIORA_PARSE_CMD": "parse ../../logs/short_xoriginalforwardedfor.log match ../../logs/short_whitelist.txt"
            },
            "args": [
                "parse",
                "../../logs/short_xoriginalforwardedfor.log",
                "match",
                "../../logs/short_whitelist.txt",
            ],
            "showLog": true
        }
    ]
}